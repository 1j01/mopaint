		"art",
		"drawing",
		"drawing-tool",
		"symmetry"
	],
	"dependencies": {
		"anypalette": "0.1.0",
		"hash.js": "1.1.5",
		"husky": "1.1.2",
		"immutable": "3.8.2",
		"localforage": "1.7.2",
		"prettier": "1.14.3",
		"pretty-quick": "1.8.0",
		"prop-types": "15.6.2",
		"react": "16.5.2",
		"react-dom": "16.5.2",
		"react-scripts": "2.1.2",
		"shortid": "2.2.13"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"pretty-quick": "pretty-quick",
		"pretty-all": "prettier \"**/*.{js,jsx,json,css}\" \"!.*/**/*\" \"!**/build/**\" --write",
		"pretty-all-preview": "prettier \"**/*.{js,jsx,json,css}\" \"!.*/**/*\" \"!**/build/**\" --list-different"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
