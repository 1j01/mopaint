{
  "name": "mopaint",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@most/create": "^2.0.1",
    "husky": "^0.14.3",
    "immutable": "^3.8.2",
    "lint-staged": "^7.1.0",
    "most": "^1.7.3",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "^1.1.4",
    "react-tap-event-plugin": "^3.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "format": "prettier --write \"src/**/*.{js,css}\""
  },
  "lint-staged": {
    "src/**/*.{js,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "useTabs": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  }
}
